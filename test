function generatePDF() {
    const userName = document.getElementById('userName').value;
    const userAddress = document.getElementById('userAddress').value;
    const pdfType = document.getElementById('pdfType').value;

    let tableContent = getTableContentByType(pdfType);

    const image = new Image();
    image.src = "mitsubishi.jpg";
    image.onload = function() {
        const firstPageContent = `
            <div class="page">
                <img src="${image.src}" width="100%">
                <h1>${pdfType}</h1>
            </div>
        `;
        const secondPageContent = `
    <div style="page-break-after: always;">
    <div class="header-info">
    Sprzedawca: "KAMAN" SP. Z O.O.
    Opiekun klienta: Beata Mularczyk
    Tel: 884996055
    Email: bmularczyk.kaman@gmail.com
</div>
<h1>OFERTA DLA KLIENTA</h1>
<div class="client-info">
    Nabywca: ${userName}
    Adres: ${userAddress}
    Tel: 606-636-459
    Email: lukaszpodsiadlooo@gmail.com
</div>
<table class="table">
    ${tableContent}
</table>
Uwagi: CENA KOŃCOWA ZA INSTALACJĘ: 45 800 BRUTTO Z 8% VAT
        WYCENA WAŻNA 14 DNI
    </div>
`;

const thirdPageContent = `
    <div>
    <table class="table">
    ${tableContent}
</table>
    </div>
`;

        // Reszta Twojego kodu...

        const content = firstPageContent + secondPageContent + thirdPageContent;

        const opt = {
            margin: 10,
            filename: `${userName}_Oferta.pdf`,
            image: { type: 'jpeg', quality: 0.98 },
            html2canvas: { scale: 2 },
            jsPDF: { unit: 'mm', format: 'a4', orientation: 'portrait' }
        };
        html2pdf().from(content).set(opt).save();
    };
}
function getTableContentByType(pdfType) {
    let tableContent = "";
    switch(pdfType) {
        case "Toshiba":
            tableContent = `...`; // Kod tabeli dla Toshiba
            break;
        case "Mitsubishi":
            tableContent = `...`; // Kod tabeli dla Mitsubishi
            break;
        case "ATLANTIC":
            tableContent = `...`; // Kod tabeli dla ATLANTIC
            break;
        // Możesz dodać więcej przypadków dla innych opcji
    }
    return tableContent;
}

















