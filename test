26.10 

document.getElementById('generatePdfButton').addEventListener('click', generatePDF);

let imageBase64;
let imageBase63;

async function loadImageData() {
    const imagesModule = await import('./images.js');
    imageBase64 = imagesModule.imageBase64;
    imageBase63 = imagesModule.imageBase63;
}

loadImageData();


function generatePDF() {
    const userName = document.getElementById('userName').value;
    const pdfType = document.getElementById('pdfType').value;

    let tableContent = getTableContentByType(pdfType);
 

    const firstPageContent = `
    <div id="page">
        <div id="top-page" style="background-image: url('${imageBase64}');">
            <div id="top-top-page">
                <img src="${imageBase63}" width="20%">
            </div>
            <div id="top-bottom-page">
                <span>Mitsubishi Zubadan 10 kW</span>
            </div>
        </div>
        <div id="bottom-page">
            <div id="bottom-top-left">
                <span>Przygotowano dla:</span>
            </div>
            <div id="bottom-top-right">
                <span>${userName}</span>
            </div>
            <div id="bottom-bottom-left">
                <span>Kontakt:</span>
            </div>
            <div id="bottom-bottom-right">
                <span>Numer telefonu: 721174625</span>
            </div>
        </div>
    </div>
`;
    const secondPageContent = `
    <div id="second-page">
        <div style="page-break-after: always;">
            <table class="table">
                ${tableContent}
            </table>
            Uwagi: CENA KOŃCOWA ZA INSTALACJĘ: 45 800 BRUTTO Z 8% VAT<br>
            WYCENA WAŻNA 14 DNI<br>
        </div>
        </div>
    `;

    const thirdPageContent = `
        <div>
            <table class="table">
                ${tableContent}
            </table>
        </div>
    `;

    const content = firstPageContent + secondPageContent + thirdPageContent;

    const opt = {
        margin: 10,
        filename: `${userName}_Oferta.pdf`,
        image: { type: 'jpeg', quality: 0.98 },
        html2canvas: { scale: 2 },
        jsPDF: { unit: 'mm', format: 'a4', orientation: 'portrait' }
    };
    html2pdf().from(content).set(opt).save();
}

